<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" opacity="0.8" prefHeight="447.0" prefWidth="697.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.millmaster.contoller.ForgotPasswordFormController">
   <children>
      <AnchorPane fx:id="AnchorPaneFogotPassword" prefHeight="447.0" prefWidth="697.0">
         <children>
            <Label layoutX="158.0" layoutY="14.0" text="Forgot Password" textFill="WHITE">
               <font>
                  <Font name="Madhura" size="48.0" />
               </font>
            </Label>
            <ComboBox fx:id="comUserName" layoutX="214.0" layoutY="159.0" prefHeight="31.0" prefWidth="270.0" promptText="Selecte User Name" styleClass="FontSize" stylesheets="@../CSS/LoginFormCSS.css" />
            <Button fx:id="btnSendOTP" layoutX="281.0" layoutY="322.0" mnemonicParsing="false" onAction="#btnSendOTPOnAction" prefHeight="39.0" prefWidth="136.0" styleClass="AddNewUser" stylesheets="@../CSS/LoginFormCSS.css" text="Send OTP" />
            <Label fx:id="lblEmail" layoutX="94.0" layoutY="386.0" opacity="0.0" text="Label" />
            <ImageView fx:id="btnClose" fitHeight="32.0" fitWidth="35.0" layoutX="651.0" layoutY="14.0" onMouseClicked="#btnCloseOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/cancel.png" />
               </image>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ImageView>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
